{
  "name": "widget-template",
  "version": "0.0.1-beta.1",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@date-io/moment": "^1.3.11",
    "@material-ui/core": "^4.5.1",
    "@material-ui/pickers": "^3.2.7",
    "fm-bridge": "^0.0.1-beta.11",
    "hoi-poi-ui": "^0.0.1-alpha.89",
    "moment": "^2.24.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "node-sass": "^4.12.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none concurrently --kill-others  \"yarn run react-start\" \"yarn run fm-cli-start\"",
    "react-start": "react-scripts start",
    "fm-cli-start": "fm-cli start",
    "build": "fm-cli set_public_url && react-scripts build",
    "build-sandbox": "fm-cli set_public_url_sandbox && react-scripts build",
    "deploy": "yarn run build && fm-cli deploy",
    "deploy-sandbox": "yarn run build-sandbox && fm-cli deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
