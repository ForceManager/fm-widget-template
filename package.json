{
  "name": "widget-template",
  "version": "0.0.1-beta.5",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@date-io/moment": "^2.6.0",
    "@material-ui/core": "^4.9.10",
    "@material-ui/pickers": "^3.2.10",
    "fm-bridge": "^0.0.1-beta.16",
    "hoi-poi-ui": "^0.0.1-alpha.129",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "node-sass": "^4.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none concurrently --kill-others  \"npm run react-start\" \"npm run fm-start\"",
    "react-start": "react-scripts start",
    "fm-start": "fm start",
    "build": "fm set_public_url && react-scripts build",
    "build-sandbox": "fm set_public_url_sandbox && react-scripts build",
    "deploy": "npm run build && fm deploy",
    "deploy-sandbox": "npm run build-sandbox && fm deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
