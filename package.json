{
  "name": "widget-template",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "fm-bridge": "^0.0.3",
    "hoi-poi-ui": "^0.0.43",
    "node-sass": "4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none concurrently --kill-others  \"npm run react-start\" \"npm run fm-start\"",
    "react-start": "react-scripts start",
    "fm-start": "fm start",
    "build": "fm set_public_url && react-scripts build",
    "build-sandbox": "fm set_public_url_sandbox && react-scripts build",
    "deploy": "npm run build && fm deploy",
    "deploy-sandbox": "npm run build-sandbox && fm deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
